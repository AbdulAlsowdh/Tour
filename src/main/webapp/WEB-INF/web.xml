<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>ASTRAVEL</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
  	<display-name>Addpackage</display-name>
  	<servlet-name>Addpackage</servlet-name>
  	<servlet-class>Addpackage</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>Addpackage</servlet-name>
  		<url-pattern>/Addpackage</url-pattern>
  </servlet-mapping>
  <servlet>
  	<display-name>Deletepackage</display-name>
  	<servlet-name>Deletepackage</servlet-name>
  	<servlet-class>Deletepackage</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>Deletepackage</servlet-name>
  		<url-pattern>/Deletepackage</url-pattern>
  </servlet-mapping>
</web-app>